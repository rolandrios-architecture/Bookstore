{
  "name": "restful-api-for-online-bookstore",
  "version": "1.0.0",
  "type": "module",
  "description": "project to create a RESTful API for an online bookstore using Node.js and Express.js with different technologies and react",
  "main": "server.mjs",
  "scripts": {
    "test": "jest --coverage",
    "test:integration": "jest --testPathPattern=tests/integration --runInBand",
    "test:integration:docker": "node scripts/run-integration-tests.js",
    "lint": "eslint . --ext .js,.mjs",
    "start": "node server.mjs",
    "dev": "nodemon server.mjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^5.3.0",
    "@as-integrations/express5": "^1.1.2",
    "bcryptjs": "^3.0.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-validator": "^7.3.1",
    "graphql": "^16.12.0",
    "graphql-tag": "^2.12.6",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.1.5",
    "mysql2": "^3.16.2",
    "prom-client": "^15.1.3",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.11"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.cjs"
    ]
  }
}
